kubectl create clusterrolebinding cluster-admin-binding \
--clusterrole=cluster-admin \
--user=qpfortier@gmail.com
￼￼
gcloud beta container node-pools create user-pool \
  --machine-type n1-standard-2 \
  --num-nodes 0 \
  --enable-autoscaling \
  --min-nodes 0 \
  --max-nodes 3 \
  --node-labels hub.jupyter.org/node-purpose=user \
  --node-taints hub.jupyter.org_dedicated=user:NoSchedule \
  --zone us-central1-b \
  --cluster mp2i

  helm upgrade --cleanup-on-fail \
  --install 1.4.2 jupyterhub/jupyterhub \
  --namespace mp2i \
  --create-namespace \
  --version=1.1.2 \
  --values config.yaml
:latest
kubectl config set-context $(kubectl config current-context) --namespace mp2i

kubectl get service --namespace mp2i

